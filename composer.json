{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "platform": {
            "php": "7.1"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "description": "Srigi HACE",
    "extra": {
        "cleaner-ignore": {
            "phpstan/phpstan": [
                "conf"
            ]
        }
    },
    "license": "MIT",
    "name": "srigi/hace",
    "require": {
        "php": ">= 7.1",
        "dg/composer-backslasher": "^0.3",
        "dg/composer-cleaner": "^2.0",
        "kdyby/console": "~2.7.1",
        "kdyby/monolog": "~1.3.2",
        "latte/latte": "~2.4.8",
        "nette/application": "~2.4.12",
        "nette/bootstrap": "~2.4.6",
        "nette/caching": "~2.5.8",
        "nette/database": "~2.4.6",
        "nette/di": "~2.4.13",
        "nette/forms": "~2.4.8",
        "nette/http": "~2.4.9",
        "nette/robot-loader": "~3.0.4",
        "nette/security": "~2.4.3",
        "nette/utils": "~2.5.2",
        "nextras/migrations": "3.1.0-rc5",
        "ramsey/uuid": "~3.7.3",
        "tracy/tracy": "~2.5.1"
    },
    "require-dev": {
        "consistence/coding-standard": "~3.4",
        "jakub-onderka/php-console-highlighter": "~0.3.2",
        "jakub-onderka/php-parallel-lint": "~1.0.0",
        "phpstan/phpstan": "~0.10.1",
        "phpstan/phpstan-strict-rules": "^0.10.1",
        "roave/security-advisories": "dev-master",
        "slevomat/coding-standard": "~4.6"
    },
    "type": "project"
}
